
# Mealie API Token
# Get this from Mealie > Settings > API Tokens
MEALIE_TOKEN=your_mealie_token_here

# Anthropic API Key
# Get from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_key_here

# Instacart Connect API
# Register at https://docs.instacart.com/connect/api/
INSTACART_ACCESS_TOKEN=your_instacart_token_here

# New Seasons Market Retailer ID
# You'll get this from Instacart after registration
INSTACART_RETAILER_ID=newseasons_retailer_id

# Your local New Seasons Market Store ID
# Find this in the Instacart API docs or during setup
INSTACART_STORE_ID=your_store_id